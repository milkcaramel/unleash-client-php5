{
    "name": "unleash-client/php5",
    "description": "unleash client for php5",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^5.4",
        "guzzlehttp/guzzle": "^6.5",
        "cache/filesystem-adapter": "^1.1",
        "lastguest/murmurhash": "^1.3",
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.19",
        "symfony/cache": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Unleash\\": "src/"
        }
    },
    "scripts": {
        "fixer": "php-cs-fixer fix --verbose --allow-risky=yes",
        "phpunit": "phpunit"
    }

}
